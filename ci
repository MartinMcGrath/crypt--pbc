#!/bin/bash
# $Id: ci,v 1.1 2006/11/11 14:38:08 jettero Exp $
# vi:syntax=gentoo-init-d:

source /sbin/functions.sh
set -e; trap "eend 997" ERR

ebegin "checking MANIFEST";
./distclean
eend $?

ebegin "checking up";
cvs up
echo "check up complete"
eend $?
 
ebegin "checking in"
cvs ci -m ci
echo "check in complete"
eend $?
